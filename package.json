{
  "name": "move-on",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "npx typeorm-ts-node-commonjs -d src/typeorm/typeorm.config.ts",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create",
    "migration:generate": "yarn typeorm:cli -- migration:generate",
    "migration:run": "yarn typeorm:cli -- migration:run",
    "migration:revert": "yarn typeorm:cli -- migration:revert",
    "migration:show": "yarn typeorm:cli -- migration:show",
    "seeder:run": "cross-env MR=seeder yarn typeorm:cli -- migration:run"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@as-integrations/fastify": "^2.1.1",
    "@aws-sdk/client-s3": "^3.433.0",
    "@aws-sdk/s3-request-presigner": "^3.433.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.7",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@types/bcrypt": "^5.0.1",
    "@types/cli-progress": "^3.11.4",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.7",
    "@willsoto/nestjs-prometheus": "^6.0.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli-progress": "^3.12.0",
    "cloudinary": "^1.41.0",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "fastify": "^4.24.3",
    "global": "^4.4.0",
    "graphql": "^16.8.1",
    "image-size": "^1.0.2",
    "ioredis": "^5.3.2",
    "mysql2": "^3.6.2",
    "passport": "^0.6.0",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
